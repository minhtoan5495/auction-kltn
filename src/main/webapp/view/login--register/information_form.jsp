
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!--  CONTENT -->
<div class="main-content-register">
	<div class="container">
		<div class="row">
			<div class="content-register-title">
				<h2 class="content-paget-title-item">Register</h2>
			</div>
			<div class="col-sm-2"></div>
			<div class="col-sm-8">
				<div class="account-information">
					<form class="new-account-form primary-form">
						<h3 class="form-heading">Nhập thông tin đăng ký</h3>
						<div class="form-content">
							<div class="form-group primary-form-group">
								<div class="row">
									<div class="col-sm-6">
										<label for="fistname">First Name</label> <input
											class="form-control input-field" type="text"
											placeholder="first name">
										<p>(ex: Mai, Lê, Trần...)</p>
									</div>
									<div class="col-sm-6">
										<label for="lastname">Last Name</label> <input
											class="form-control input-field" type="text"
											placeholder="last name">
										<p>(ex: Sơn, Messi...)</p>
									</div>
								</div>
								<div class="row">
									<div class="col-sm-4">
										<label for="year">Năm sinh</label> <input
											class="form-control input-field">
										<p>(Vui lòng nhập đúng năm sinh)</p>
									</div>
									<div class="col-sm-4">
										<label for="year">Tháng sinh</label> <input
											class="form-control input-field">
										<p>(Vui lòng nhập đúng tháng sinh)</p>
									</div>
									<div class="col-sm-4">
										<label for="year">Ngày sinh</label> <input
											class="form-control input-field">
										<p>(Vui lòng nhập đúng ngày sinh)</p>
									</div>
								</div>
								<label for="email">Email</label> <input
									class="form-control input-field" type="email">
								<p>* Bắt buộc</p>
								<label for="Phone">Phone</label> <input
									class="form-control input-field" type="tel"> <label
									for="email">Age</label> <input class="form-control input-field"
									type="number">
								<div class="submit-button">
									<a href="#" class="btn submit-btn"> <span><i
											class="fa fa-user submit-icon"></i> Create new account </span>
									</a>
								</div>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- END CONTENT -->